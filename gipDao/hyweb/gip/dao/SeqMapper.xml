<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="hyweb.gip.dao.SeqMapper" >
  <resultMap id="BaseResultMap" type="hyweb.gip.pojo.mybatis.table.Seq" >
    <id column="name" property="name" jdbcType="NVARCHAR" />
    <result column="val" property="val" jdbcType="BIGINT" />
  </resultMap>
  <sql id="Base_Column_List" >
    name, val
  </sql>
  <select id="getSeq" parameterType="java.lang.String" statementType="CALLABLE" resultType="java.lang.Long">
 	{CALL sp_getBaphiqIdentity(
        #{name,mode=IN,jdbcType=NVARCHAR})
        }
  </select>
   <select id="getNextOrderNum"  statementType="CALLABLE" resultType="java.lang.String">
 	{CALL sp_getNextOrderNum}
  </select>
</mapper>